<div class="columns-1-2-1" id="main-content" role="main">

    <div class="portlet-layout row-fluid" style="min-height: 300px;">
        <div class="portlet-column portlet-column-only" id="top">
            $processor.processColumn("top", "portlet-column-content portlet-column-only")
        </div>
    </div>

    <div class="portlet-layout row-fluid" style="min-height: 300px;">
        <div class="portlet-column portlet-column-only fus-section fus-feature fus-white-bg metro grain" id="feature1" style="background-color: #4C1919!important;min-height: 300px;">
            $processor.processColumn("feature1", "portlet-column-content portlet-column-only")
        </div>
    </div>

    <!--facultades-->    
    <div class="portlet-layout row-fluid" style="min-height: 300px;">
        <div class="portlet-column portlet-column-only fus-section fus-white-bg metro" id="feature2">
            $processor.processColumn("feature2", "portlet-column-content portlet-column-only")
        </div>
    </div>



    <!--ultimas noticias-->
    <div class="portlet-layout row-fluid" style="min-height: 300px;">
        <div class="portlet-column portlet-column-only fus-section fus-white-bg metro" id="feature3" style="border-top: green solid 4px; padding: 0px;min-height: 300px;" >
            $processor.processColumn("feature3", "portlet-column-content portlet-column-only")
        </div>
    </div>


    <!--social wrap-->
    <div class="grain fus-section portlet-layout" style="min-height: 150px;">
        <div class="portlet-column portlet-column-only" id="socialxx">
            $processor.processColumn("socialxx", "portlet-column-content portlet-column-only")
        </div>        
    </div>

    <div id="footer">
        <div class="container" style="min-width: 330px;background: none repeat scroll 0% 0% #222;" >
            <div class="portlet-layout row-fluid container featuredcontainer clearfix" style="min-height: 300px;">
                <div class="portlet-column portlet-column-only" id="footer2">
                    $processor.processColumn("footer2", "portlet-column-content portlet-column-only")
                </div>
            </div>
            <div class="clear"></div>
        </div>
    </div>   
</div>